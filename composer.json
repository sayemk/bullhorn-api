{
    "name": "justijndepover/bullhorn-api",
    "description": "PHP client for the Bullhorn API",
    "license": "MIT",
    "authors": [
        {
            "name": "Justijn Depover",
            "email": "justijndepover@gmail.com"
        }
    ],
    "keywords": [
        "php",
        "bullhorn",
        "api"
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.22",
        "justijndepover/php-check": "^0.12.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Sayemk\\Bullhorn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sayemk\\Bullhorn\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
